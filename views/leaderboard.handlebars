<div>
    <h1 id="info-title">Leaderboard</h1>
    <h3>See where you rank below!</h3>
    <div class="leaderboard">
        <table>
            <tr>
                <th id = "section">Rank</th>
                <th id="section">Username</th>
                <th id="section">Points</th>
            </tr>
        {{#each users}}
            <tr>
                <td>#{{math @index "+" 1}}</td>
                <td>{{this.username}}</td>
                <td>{{this.points}}</td>
            </tr>
        {{/each}}
        </table>
    </div>
</div>