<main>
    <p>hi</p>
    <div class="attraction-holder">
    {{#each attractions}}
    <div class="attractions">
        <div class="content">
            <div class="contains">
                <p class="next">Name of Event:</p>
                <p>{{this.attractionName}}</p>
            </div>
            <div class="contains">
                <p class="next">Description: </p>
                <p>{{this.description}}</p>
            </div>
            <div class="contains">
                <p class="next">Date of event:</p>
                <p> {{this.date}}</p>
            </div>
            <div class="contains">
                <p class="next">Start Time: </p>
                <p>{{this.startTime}}</p>
            </div>
            <div class="contains">
                <p class="next">End Time:</p>
                <p>{{this.end_time}}</p>
            </div>
            <div class="contains">
                <p class="next">Points offered:</p>
                <p>{{this.pointsOffered}}</p>
            </div>
            <div class="contains">
                <p class="next">Bonus points:</p>
                <p>{{this.bonusPoints}}</p>
            </div>
        </div>
    </div>
    {{/each}}
</div>
<div>
</main>